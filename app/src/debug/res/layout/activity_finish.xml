<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.lonixchu.doctor.Finish">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="260dp"
        android:background="@drawable/doctorhomeedited"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/frameLayout">
    </FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@id/frameLayout">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/congrat"
        android:layout_below="@+id/frameLayout"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/buttonA"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_gravity="center|bottom"
        android:layout_marginBottom="70dp"
        android:padding="12dp"
        android:clickable="true"
        app:backgroundTint="?attr/colorBackgroundFloating"
        app:srcCompat="@drawable/homereal" />

    <TextView
        android:id="@+id/Home"
        android:layout_width="150dp"
        android:layout_height="26dp"
        android:layout_gravity="center|bottom"
        android:layout_marginBottom="35dp"
        android:text="Home"
        android:textAlignment="center"
        android:textSize="20dp" />

</FrameLayout>
</RelativeLayout>
